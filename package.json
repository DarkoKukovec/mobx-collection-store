{
  "name": "mobx-collection-store",
  "version": "1.0.0",
  "description": "Data collection store for MobX",
  "main": "dist/store.js",
  "typings": "typings.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "build:test": "webpack --config webpack.test.js",
    "build:add": "npm run build && git add dist/store.js",
    "run:test": "mocha --require source-map-support/register dist/test.js",
    "test": "npm run build:test && npm run run:test",
    "precommit": "npm test && npm run build:add"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinum/mobx-collection-store.git"
  },
  "keywords": [
    "mobx",
    "collection",
    "store",
    "observable"
  ],
  "bugs": {
    "url": "https://github.com/infinum/mobx-collection-store/issues"
  },
  "homepage": "https://github.com/infinum/mobx-collection-store#readme",
  "author": "Infinum <javascript.team@infinum.co>",
  "contributors": [
    {
      "name": "Darko Kukovec",
      "email": "darko@infinum.co"
    }
  ],
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.16.0",
    "chai": "^3.5.0",
    "husky": "^0.12.0",
    "mobx": "^2.7.0",
    "mocha": "^3.2.0",
    "mocha-typescript": "^1.0.15",
    "source-map-support": "^0.4.8",
    "typescript": "^2.1.4",
    "webpack": "^2.2.0-rc.3"
  },
  "peerDependencies": {
    "mobx": "^2.7.0 || ^3"
  }
}

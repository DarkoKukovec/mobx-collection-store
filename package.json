{
  "name": "mobx-collection-store",
  "version": "1.0.0",
  "description": "Data collection store for MobX",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "ntsc -p ./src",
    "build:add": "npm run build && git add dist/",
    "test": "nyc _mocha",
    "watch": "nodemon -e .ts -i node_modules -i dist -i coverage -x _mocha",
    "precommit": "npm test && npm run build:add"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinum/mobx-collection-store.git"
  },
  "keywords": [
    "mobx",
    "collection",
    "store",
    "observable"
  ],
  "bugs": {
    "url": "https://github.com/infinum/mobx-collection-store/issues"
  },
  "homepage": "https://github.com/infinum/mobx-collection-store#readme",
  "author": "Infinum <javascript.team@infinum.co>",
  "contributors": [
    {
      "name": "Darko Kukovec",
      "email": "darko@infinum.co"
    }
  ],
  "nyc": {
    "extensions": [
      ".ts"
    ],
    "include": [
      "lib/**/*.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "devDependencies": {
    "@types/mocha": "^2.2.35",
    "@types/node": "^6.0.55",
    "chai": "^3.5.0",
    "husky": "^0.12.0",
    "mobx": "^2.7.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "ntypescript": "^1.201609302242.1",
    "nyc": "^10.0.0",
    "ts-node": "^2.0.0",
    "typescript": "^2.1.4"
  },
  "peerDependencies": {
    "mobx": "^2.7.0 || ^3"
  }
}
